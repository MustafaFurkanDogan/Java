package mukemmelSayı;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number = 5 ;
		for(int i=1 ; i<=number; i++ ) {
			if (number % i == 0 ) {
				System.out.println("Mükemmel sayıdır");
				
			} else {
			System.out.println("Mükemmel sayı değildir");	
			}
		}

	}

}
